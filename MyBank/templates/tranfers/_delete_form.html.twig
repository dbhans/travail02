<form method="post" action="{{ path('tranfers_delete', {'id': tranfer.id , 'acc': acc}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tranfer.id) }}">
    <button class="btn">Delete</button>
</form>
